{
  "name": "anime-api",
  "version": "1.0.0",
  "description": "A restful api to provide all information about anime",
  "main": "./server.js",
  "scripts": {
    "start": "node ./server.js",
    "dev": "nodemon server.js",
    "build-worker": "esbuild src/worker.js --bundle --format=esm --outfile=dist/worker.js",
    "deploy-worker": "wrangler deploy",
    "dev-worker": "wrangler dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Sayan Das",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "cheerio": "^1.0.0-rc.12",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.2.0",
    "express": "^4.21.2",
    "image-pixels": "^2.2.2",
    "jsonwebtoken": "^9.0.2"
  },
  "type": "module",
  "devDependencies": {
    "nodemon": "^3.1.10",
    "wrangler": "^3.0.0",
    "hono": "^4.0.0",
    "esbuild": "^0.19.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/itzzzme/anime-api.git"
  }
}
